<b:includable id='g:image'>
  <b:if cond='data:src'>
    <b:tag name='img' expr:alt="data:alt ?: data:messages.image">
      <b:attr name='src' expr:value='(data:src contains "blogger.googleusercontent.com/img/a/" and data:src.isResizable) ? resizeImage(data:src, (data:resize ?: data:skin.vars.t_resizeImage), (data:ratio ?: data:skin.vars.t_ratioImage) ) + (data:params ? "-" + data:params : "-" + data:skin.vars.t_imageParams) : resizeImage(data:src, (data:resize ?: data:skin.vars.t_resizeImage), (data:ratio ?: data:skin.vars.t_ratioImage) )'/>
      <b:attr name='width' value='100%'/>
      <b:attr name='height' value='100%'/>
      <b:attr cond='data:lazy' name='loading' value='lazy'/>
    </b:tag>
    <b:else/>
    <img expr:src='data:skin.vars.t_defaultImage' />
  </b:if>
</b:includable>